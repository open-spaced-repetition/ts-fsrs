{
  "$schema": "./node_modules/turbo/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@open-spaced-repetition/binding#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["NAPI_TARGET"]
    },
    "check": {
      "outputs": []
    },
    "check::fix": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "@open-spaced-repetition/binding-test#test": {
      "dependsOn": [],
      "outputs": []
    },
    "test::coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "@open-spaced-repetition/binding-test#test::coverage": {
      "dependsOn": [],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs": {
      "dependsOn": ["build"],
      "outputs": ["docs/**"]
    }
  }
}
