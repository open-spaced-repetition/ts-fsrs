{
  "$schema": "./node_modules/turbo/schema.json",
  "globalDependencies": ["package.json"],
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@open-spaced-repetition/binding#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["NAPI_TARGET"],
      "passThroughEnv": ["ANDROID_NDK_LATEST_HOME"]
    },
    "check": {
      "outputs": [],
      "env": ["TURBO_PLATFORM"]
    },
    "check:fix": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "@open-spaced-repetition/binding-test#test": {
      "dependsOn": [],
      "outputs": []
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "@open-spaced-repetition/binding-test#test:coverage": {
      "dependsOn": [],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs": {
      "dependsOn": ["build"],
      "outputs": ["docs/**"]
    }
  }
}
