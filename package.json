    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishiko732/ts-fsrs.git"
  },
  "bugs": {
    "url": "https://github.com/ishiko732/ts-fsrs/issues"
  },
  "homepage": "https://github.com/ishiko732/ts-fsrs#readme"
}
