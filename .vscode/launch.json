{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "ts-fsrs debug",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "env": {
        // "DEBUG": "*",
      },
      "outputCapture": "std",
      "runtimeExecutable": "tsx",
      "restart": true,
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["watch","${workspaceFolder}/debug/index.ts"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "port": 9229,
      "skipFiles": [
        // Node.js internal core modules
        "<node_internals>/**",

        // Ignore all dependencies (optional)
        "${workspaceFolder}/node_modules/**"
      ]
    }
  ]
}
